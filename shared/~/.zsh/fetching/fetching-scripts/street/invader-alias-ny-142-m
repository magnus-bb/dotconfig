#!/usr/bin/env bash

# Alias NY-142 by Invader (2014)
#
# Depicts R2D2 from Star Wars. 
#
# The work was sold on March 4th, 2016 at  Christie's New York during 
# their First Open: Post-War and Contemporary Art auction for $60,000.
#
# Author: rob o'leary

f=3 b=4

for j in f b; do
  for i in {0..7}; do
    printf -v $j$i %b "\e[${!j}${i}m"
  done
done

for i in {0..7}; do
    printf -v g$i %b "\e[9${i}m"
done

bd=$'\e[1m'
rt=$'\e[0m'
iv=$'\e[7m'

